<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="document">
    <qtext qname="(Breadcrumb = BcRoot \+ &quot;.*?)\n(.*?&quot;)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$1$2" param2=""/>
</dwquery>
