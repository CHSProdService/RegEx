<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="document">
    <qtext qname="Breadcrumb\s*=\s*&quot;BcRoot((&lt;li&gt;&lt;a href='.*?'&gt;.*?&lt;/a&gt;&lt;/li&gt;){1,})?&lt;li( class='active')&gt;(.*?)&lt;/li&gt;&quot;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="Breadcrumb = BcRoot + &quot;$1<li class='active'>$4</li>&quot;" param2=""/>
</dwquery>
