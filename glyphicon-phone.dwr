<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="document">
    <qtext qname="(\s*&lt;span class=&quot;glyphicon glyphicon-phone&quot;&gt;&lt;\/span&gt;)?(\s*)(\+?(\d?)-?\s*\(?(\d{3})\)?-?\s*(\d{3})-?\s*(\d{4}))" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$2<span class=&quot;glyphicon glyphicon-phone&quot;></span> $4 ($5) $6-$7" param2=""/>
</dwquery>
