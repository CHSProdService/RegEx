<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="selectedFiles">
    <qtext qname="(var breadcrumb = \&quot;(?:(?:&lt;li&gt;&lt;a href=\'.*?\'&gt;.*?&lt;\/a&gt;&lt;\/li&gt;){1,})?)&lt;li( class=\'active\')?&gt;.*?&lt;\/li&gt;(\&quot;;)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$1$2" param2=""/>
</dwquery>
